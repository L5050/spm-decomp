{
    "[c]": {
        "files.encoding": "utf8"
    },
    "[cpp]": {
        "files.encoding": "utf8"
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.associations": {
        "*.inc": "cpp",
        "*.h": "cpp",
        "*.c": "cpp"
    },
    "search.useIgnoreFiles": false,
    "search.exclude": {
        "build/*/config.json": true,
        "build/**/*.MAP": true,
        "build.ninja": true,
        ".ninja_*": true,
        "objdiff.json": true,
        ".mypy_cache": true
    }
}
